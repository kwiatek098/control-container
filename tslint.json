{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-empty":false,
    "ordered-imports":false,
    "no-console": {
        "severity": "warning",
        "options": [
            "debug",
            "info",
            "log",
            "time",
            "timeEnd",
            "trace"
        ]
    }
},
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
